{
  "name": "shy-dew-c896",
  "compatibility_date": "2025-06-18", 
  "main": "dist/worker.js",
  "assets": {
    "directory": "assets",
    "not_found_handling": "404-page"
  },
  "build": {
    "command": "npm install esbuild && node assets/js/esbuild.config.js"
  },
  "no_bundle": true,
  "bindings": [
    { "type": "kv_namespace", "name": "__STATIC_CONTENT", "namespace_id": "your-kv-namespace-id" },
    { "type": "text_blob", "name": "__STATIC_CONTENT_MANIFEST", "part": "manifest" }
]
}

